@model ResetPasswordDto
<main>
    <!-- Login Part Start -->
    <section id="Login">
        <div class="container">

            <div class="login-form">
                <a style="font-size:20px" href="#" class="active">ResetPassword</a>

                <form method="post" class="woocommerce-form woocommerce-form-register register">
                    <div class="alert alert-danger" asp-validation-summary="ModelOnly"></div>
                    <input hidden asp-for="Email">
                    <input hidden asp-for="Token">
                    <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                        <label for="reg_username">Please enter your new Password&nbsp;<span class="required">*</span></label>
                        <input type="password"  asp-for="Password"  class="zhr2 form-control woocommerce-Input woocommerce-Input--text input-text" placeholder="Password" id="reg_username"  value="">
                        <span asp-validation-for="Password" style="color:red"></span>
                    </p>
                    <p class="woocommerce-form-row form-row">
                        <button type="submit" value="submit" class="primary-btn">Reset Password</button>
                    </p>
                </form>
            </div>
        </div>
    </section>
    <!-- Login Part End -->
</main>

